<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns     = "http://www.w3.org/ns/widgets" xmlns:gap = "http://phonegap.com/ns/1.0" id="kz.elecor" version="3">
    <name>Elecor</name>
    <description>111</description>
    <author href="http://phonegap.com" email="elegant4444@gmail.com">MaksimMorozov</author>

    <content src="index.html" />

    <preference name="DisallowOverscroll"         value="false" />
<preference name="android-minSdkVersion"      value="15" />
<preference name="android-targetSdkVersion" value="26" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="handset" />

<plugin name="cordova-plugin-device" spec="1.1.6" />
<plugin name="cordova-plugin-device-orientation" spec="1.0.7" />
<plugin name="cordova-plugin-inappbrowser" spec="1.3.0" />
<plugin name="cordova-plugin-network-information" spec="1.2.1" />
<plugin name="cordova-plugin-whitelist" spec="1.2.2" />
<plugin name="onesignal-cordova-plugin"  spec="^2.4.1" source="npm" />
      
<preference name="phonegap-version" value="cli-6.5.0" />
<preference name="android-build-tool" value="gradle" />

    <icon src="www/res/icon/ios/1024.png" />
    <platform name="android">
     	<icon src="www/androidIcon/mdpi_ic_launcher.png" density="mdpi" />
     	<icon src="www/androidIcon/hdpi_ic_launcher.png" density="hdpi" />
     	<icon src="www/androidIcon/xhdpi_ic_launcher.png" density="xhdpi" />
     	<icon src="www/androidIcon/xxhdpi_ic_launcher.png" density="xxhdpi" />
     	<icon src="www/androidIcon/xxxhdpi_ic_launcher.png" density="xxxhdpi" />
    </platform>

    <platform name="ios">
        <icon   src="www/res/icon/ios/Icon-App-57x57@1x.png"                         platform="ios" width="57"   height="57" />
        <icon   src="www/res/icon/ios/Icon-App-57x57@2x.png"                      platform="ios" width="114"  height="114" />
        <icon   src="www/res/icon/ios/Icon-App-40x40@1x.png"                      platform="ios" width="40"   height="40" />
        <icon   src="www/res/icon/ios/Icon-Small-40@2x.png"                   platform="ios" width="80"   height="80" />
        <icon   src="www/res/icon/ios/Icon-Small-50x50@1x.png"                      platform="ios" width="50"   height="50" />
        <icon   src="www/res/icon/ios/Icon-Small-50@2x.png"                   platform="ios" width="100"  height="100" />
        <icon   src="www/res/icon/ios/Icon-App-60x60@2x.png"                   platform="ios" width="120"  height="120" />
        <icon   src="www/res/icon/ios/Icon-App-60x60@3x.png"                   platform="ios" width="180"  height="180" />
        <icon   src="www/res/icon/ios/Icon-App-72x72@1x.png"                      platform="ios" width="72"   height="72" />
        <icon   src="www/res/icon/ios/Icon-App-72x72@2x.png"                   platform="ios" width="144"  height="144" />
        <icon   src="www/res/icon/ios/Icon-App-76x76@1x.png"                      platform="ios" width="76"   height="76" />
        <icon   src="www/res/icon/ios/Icon-App-76x76@2x.png"                   platform="ios" width="152"  height="152" />
        <icon   src="www/res/icon/ios/Icon-App-29x29@1x.png"                   platform="ios" width="29"   height="29" />
        <icon   src="www/res/icon/ios/Icon-App-29x29@2x.png"                platform="ios" width="58"   height="58" />
        <icon   src="www/res/icon/ios/Icon-App-29x29@3x.png"                platform="ios" width="87"   height="87" />
        <icon   src="www/res/icon/ios/icon-small@3x.png"             platform="ios" width="87"   height="87" />
        <icon   src="www/res/icon/ios/1024.png"                platform="ios" width="1024"   height="1024" />
        <splash src="www/res/screen/ios/Default-568h@2x~iphone.png"     platform="ios" width="640"  height="1136" />
        <splash src="www/res/screen/ios/Default-667h.png"               platform="ios" width="750"  height="1334" />
        <splash src="www/res/screen/ios/Default-736h.png"               platform="ios" width="1242" height="2208" />
        <splash src="www/res/screen/ios/Default-Landscape-736h.png"     platform="ios" width="2208" height="1242" />
        <splash src="www/res/screen/ios/Default-Landscape@2x~ipad.png"  platform="ios" width="2048" height="1536" />
        <splash src="www/res/screen/ios/Default-Landscape~ipad.png"     platform="ios" width="1024" height="768" />
        <splash src="www/res/screen/ios/Default-Portrait@2x~ipad.png"   platform="ios" width="1536" height="2048" />
        <splash src="www/res/screen/ios/Default-Portrait~ipad.png"      platform="ios" width="768"  height="1024" />
        <splash src="www/res/screen/ios/Default@2x~iphone.png"          platform="ios" width="640"  height="960" />
        <splash src="www/res/screen/ios/Default~iphone.png"             platform="ios" width="320"  height="480" />
    </platform>

 

    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
